<NotepadPlus>
    <UserLang name="Maru Markdown" ext="md markdown" udlVersion="2.1">
        <Settings>
            <Global caseIgnored="yes" allowFoldOfComments="no" foldCompact="no" forcePureLC="0" decimalSeparator="0" />
            <Prefix Keywords1="yes" Keywords2="no" Keywords3="no" Keywords4="no" Keywords5="no" Keywords6="no" Keywords7="no" Keywords8="no" />
        </Settings>
        <KeywordLists>
            <Keywords name="Comments">00 01 02 03 04</Keywords>
            <Keywords name="Numbers, prefix1"></Keywords>
            <Keywords name="Numbers, prefix2"></Keywords>
            <Keywords name="Numbers, extras1"></Keywords>
            <Keywords name="Numbers, extras2"></Keywords>
            <Keywords name="Numbers, suffix1">.</Keywords>
            <Keywords name="Numbers, suffix2">.</Keywords>
            <Keywords name="Numbers, range"></Keywords>
            <Keywords name="Operators1">@ &lt; &gt; \&lt; \&gt; \\ \` \* \_ \{ \} \[ \] \( \) \# \+ \- \. \! \~ \| |:-: |:--: |:---: | :-: :--: :---: :|: |: :|</Keywords>
            <Keywords name="Operators2">- + :- :-- :--- :</Keywords>
            <Keywords name="Folders in code1, open"></Keywords>
            <Keywords name="Folders in code1, middle"></Keywords>
            <Keywords name="Folders in code1, close"></Keywords>
            <Keywords name="Folders in code2, open"></Keywords>
            <Keywords name="Folders in code2, middle"></Keywords>
            <Keywords name="Folders in code2, close"></Keywords>
            <Keywords name="Folders in comment, open"></Keywords>
            <Keywords name="Folders in comment, middle"></Keywords>
            <Keywords name="Folders in comment, close"></Keywords>
            <Keywords name="Keywords1">http:// (http:// https:// (https:// mailto: (mailto: ftp:// (ftp:// ftps:// (ftps:// (/ /</Keywords>
            <Keywords name="Keywords2">==== ----</Keywords>
            <Keywords name="Keywords3"></Keywords>
            <Keywords name="Keywords4"></Keywords>
            <Keywords name="Keywords5"></Keywords>
            <Keywords name="Keywords6"></Keywords>
            <Keywords name="Keywords7"></Keywords>
            <Keywords name="Keywords8"></Keywords>
            <Keywords name="Delimiters">00``` 00` 01 02``` 02((EOL `)) 03![ 03[ 04\ 05] 05] 06((**)) 06((__)) 07\ 08((EOL **)) 08((EOL __)) 09((*)) 09((_)) 10\ 11((EOL *)) 11((EOL _)) 12#### 13\ 14((EOL)) 15### 16\ 17((EOL)) 18## 19\ 20((EOL)) 21# 22\ 23((EOL))</Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" fgColor="CFCCBB" bgColor="383839" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="COMMENTS" fgColor="89C9D3" bgColor="112435" fontName="" fontStyle="0" nesting="1025" />
            <WordsStyle name="LINE COMMENTS" fgColor="89C9D3" bgColor="112435" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="NUMBERS" fgColor="CFCCBB" bgColor="383839" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS1" fgColor="2B91AF" bgColor="383839" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS2" fgColor="FF0000" bgColor="383839" fontName="" fontStyle="1" nesting="0" />
            <WordsStyle name="KEYWORDS3" fgColor="7D92A6" bgColor="383839" fontName="" fontStyle="7" nesting="0" />
            <WordsStyle name="KEYWORDS4" fgColor="CFCCBB" bgColor="383839" fontName="" fontStyle="1" nesting="0" />
            <WordsStyle name="KEYWORDS5" fgColor="CFCCBB" bgColor="383839" fontName="" fontStyle="2" nesting="0" />
            <WordsStyle name="KEYWORDS6" fgColor="CFCCBB" bgColor="383839" fontName="" fontStyle="3" nesting="0" />
            <WordsStyle name="KEYWORDS7" fgColor="CFCCBB" bgColor="383839" fontName="" fontStyle="3" nesting="0" />
            <WordsStyle name="KEYWORDS8" fgColor="CFCCBB" bgColor="383839" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="OPERATORS" fgColor="A887CD" bgColor="383839" fontName="" fontStyle="1" nesting="0" />
            <WordsStyle name="FOLDER IN CODE1" fgColor="7D92A6" bgColor="383839" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="FOLDER IN CODE2" fgColor="333333" bgColor="383839" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="FOLDER IN COMMENT" fgColor="333333" bgColor="383839" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS1" fgColor="89C9D3" bgColor="112435" fontName="" fontStyle="0" nesting="0" />
            <WordsStyle name="DELIMITERS2" fgColor="EC69A6" bgColor="383839" fontName="" fontStyle="1" nesting="0" />
            <WordsStyle name="DELIMITERS3" fgColor="89C9D3" bgColor="383839" fontName="" fontStyle="1" nesting="1026" />
            <WordsStyle name="DELIMITERS4" fgColor="65CA67" bgColor="383839" fontName="" fontStyle="2" nesting="1026" />
            <WordsStyle name="DELIMITERS5" fgColor="F4DD0B" bgColor="383839" fontName="" fontStyle="1" fontSize="12" nesting="0" />
            <WordsStyle name="DELIMITERS6" fgColor="F4DD0B" bgColor="383839" fontName="" fontStyle="1" fontSize="14" nesting="0" />
            <WordsStyle name="DELIMITERS7" fgColor="F4DD0B" bgColor="383839" fontName="" fontStyle="1" fontSize="16" nesting="0" />
            <WordsStyle name="DELIMITERS8" fgColor="F4DD0B" bgColor="383839" fontName="" fontStyle="1" fontSize="18" nesting="0" />
        </Styles>
    </UserLang>
</NotepadPlus>
